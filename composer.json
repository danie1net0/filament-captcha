{
  "name": "danie1net0/filament-captcha",
  "description": "Multi-provider captcha integration for Filament forms (hCaptcha, reCAPTCHA v2/v3, Turnstile)",
  "keywords": [
    "filament",
    "captcha",
    "hcaptcha",
    "recaptcha",
    "turnstile",
    "laravel"
  ],
  "homepage": "https://github.com/danie1net0/filament-captcha",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rodrigues",
      "email": "daniel@example.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "filament/filament": "^3.0|^4.0",
    "spatie/laravel-package-tools": "^1.16"
  },
  "require-dev": {
    "driftingly/rector-laravel": "^2.0",
    "larastan/larastan": "^2.9|^3.0",
    "laravel/pint": "^1.0",
    "orchestra/testbench": "^9.0|^10.0",
    "pestphp/pest": "^2.0|^3.0|^4.0",
    "pestphp/pest-plugin-laravel": "^2.0|^3.0|^4.0",
    "rector/rector": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Ddr\\FilamentCaptcha\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ddr\\FilamentCaptcha\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "prepare-husky": "npx husky install",
    "prettier:fix": "npx prettier --write .",
    "test": "pest",
    "test:coverage": "pest --coverage",
    "lint": "pint --test",
    "lint:fix": "pint",
    "analyse": "phpstan analyse --memory-limit=-1",
    "refactor": "rector process",
    "refactor:dry": "rector process --dry-run",
    "check": [
      "@lint",
      "@analyse",
      "@refactor:dry",
      "@test"
    ],
    "format": [
      "@prettier:fix",
      "@refactor",
      "@lint:fix"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ddr\\FilamentCaptcha\\FilamentCaptchaServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
